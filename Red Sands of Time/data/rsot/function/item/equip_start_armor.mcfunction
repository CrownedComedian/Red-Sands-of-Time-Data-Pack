# Gives the player full leather armor and all items to start with
$item replace entity @s armor.head with minecraft:leather_helmet[minecraft:enchantments={"minecraft:vanishing_curse":1},minecraft:enchantment_glint_override=false,minecraft:unbreakable={},minecraft:tooltip_display={hidden_components:["minecraft:enchantments","minecraft:unbreakable","minecraft:trim"]},minecraft:trim={material:"rsot:$(team)",pattern:"minecraft:dune"}]
item replace entity @s armor.chest with minecraft:leather_chestplate[minecraft:enchantments={"minecraft:vanishing_curse":1},minecraft:enchantment_glint_override=false,minecraft:unbreakable={},minecraft:tooltip_display={hidden_components:["minecraft:enchantments","minecraft:unbreakable","minecraft:trim"]}]
item replace entity @s armor.legs with minecraft:leather_leggings[minecraft:enchantments={"minecraft:vanishing_curse":1},minecraft:enchantment_glint_override=false,minecraft:unbreakable={},minecraft:tooltip_display={hidden_components:["minecraft:enchantments","minecraft:unbreakable","minecraft:trim"]}]
$item replace entity @s armor.feet with minecraft:leather_boots[minecraft:enchantments={"minecraft:vanishing_curse":1},minecraft:enchantment_glint_override=false,minecraft:unbreakable={},minecraft:tooltip_display={hidden_components:["minecraft:enchantments","minecraft:unbreakable","minecraft:trim"]},minecraft:trim={material:"rsot:$(team)",pattern:"minecraft:coast"}]
