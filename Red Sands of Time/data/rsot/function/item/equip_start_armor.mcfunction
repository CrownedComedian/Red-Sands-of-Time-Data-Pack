# Gives the player full leather armor and all items to start with
$item replace entity @s armor.head with minecraft:leather_helmet[minecraft:enchantments={"levels":{"minecraft:vanishing_curse":1},"show_in_tooltip":false},minecraft:enchantment_glint_override=false,minecraft:unbreakable={"show_in_tooltip":false},minecraft:trim={material:"rsot:$(team)", pattern:"minecraft:dune"}]
item replace entity @s armor.chest with minecraft:leather_chestplate[minecraft:enchantments={"levels":{"minecraft:vanishing_curse":1},"show_in_tooltip":false},minecraft:enchantment_glint_override=false,minecraft:unbreakable={"show_in_tooltip":false}]
item replace entity @s armor.legs with minecraft:leather_leggings[minecraft:enchantments={"levels":{"minecraft:vanishing_curse":1},"show_in_tooltip":false},minecraft:enchantment_glint_override=false,minecraft:unbreakable={"show_in_tooltip":false}]
$item replace entity @s armor.feet with minecraft:leather_boots[minecraft:enchantments={"levels":{"minecraft:vanishing_curse":1},"show_in_tooltip":false},minecraft:enchantment_glint_override=false,minecraft:unbreakable={"show_in_tooltip":false},minecraft:trim={material:"rsot:$(team)", pattern:"minecraft:coast"}]
